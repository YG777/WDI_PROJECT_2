<h1>New Wine</h1>
<p>newejs wines</p>
<form method="POST" action="/wines">
  <div class="form-group">
    <label for="product">Wine Name</label>
    <input type="text" name="product" id="product" placeholder="wine name" class="form-control">
  </div>
  <div class="form-group">
    <label for="winery">Winery</label>
    <input type="text" name="winery" id="winery" placeholder="winery" class="form-control">
  </div>
  <div class="form-group">
    <label for="description">Tasting Notes</label>
    <textarea name="description" id="description" placeholder="tasting notes" class="form-control"></textarea>
  </div>
  
<form class="form-group" action="/wines" method="post">
    <button class="btn btn-primary">Add</button>
</form>


