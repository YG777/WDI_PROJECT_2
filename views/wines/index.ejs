<% if (locals.isLoggedIn) { %>
   <h1>Your Wine List</h1>
    <a href="/wines/new" class="btn btn-primary">Add New</a>
        <% } else { %>
          <h1>Members favorites</h1>
        <% } %>

<p>indexejs wines</p>
 
 <div class="row">
  <% wines.forEach((wine) => { %>
    <div class="col-md-4">
      <div class="card">
        <div class="card-img-top" style="background-image:url(<%= wine.images %>)" alt="<%= wine.product %>"></div>
        <div class="card-block">
          <a href="/wines/<%= wine.id %>">
            <h4 class="card-title"><%= wine.product %></h4>
          </a>
          <p class="card-text"><%= wine.winery %></p>   
        </div>
      </div>
    </div>
<% }) %>
 </div> 
 


